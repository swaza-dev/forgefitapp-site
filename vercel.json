{
  "rewrites": [
    {
      "source": "/privacy-policy",
      "destination": "/privacy/en/"
    },
    {
      "source": "/privacy-policy.html",
      "destination": "/privacy/en/"
    },
    {
      "source": "/:lang(en|es|pt|de|fr|it|ja|ko|zh|ru|tr)/privacy",
      "destination": "/privacy/:lang/"
    },
    {
      "source": "/:lang(en|es|pt|de|fr|it|ja|ko|zh|ru|tr)/privacy/",
      "destination": "/privacy/:lang/"
    },
    {
      "source": "/:lang(en|es|pt|de|fr|it|ja|ko|zh|ru|tr)/terms",
      "destination": "/terms/:lang/"
    },
    {
      "source": "/:lang(en|es|pt|de|fr|it|ja|ko|zh|ru|tr)/terms/",
      "destination": "/terms/:lang/"
    },
    {
      "source": "/:lang(en|es|pt|de|fr|it|ja|ko|zh|ru|tr)",
      "destination": "/index.html"
    },
    {
      "source": "/:lang(en|es|pt|de|fr|it|ja|ko|zh|ru|tr)/",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/en"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

